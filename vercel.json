{
    "builds": [
        {
            "src": "SEMRproject/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime":"python3.8.8" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "SEMRproject/wsgi.py"
        }
    ]
}
